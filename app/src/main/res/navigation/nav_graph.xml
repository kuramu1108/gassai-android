<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/nav_graph"
            app:startDestination="@id/homeFragment">
    <fragment android:id="@+id/homeFragment" android:name="com.pocraft.gassai.view.fragment.FeedFragment"
              android:label="FeedFragment">
        <action android:id="@+id/action_homeFragment_to_timeTableFragment"
                app:destination="@id/timeTableFragment"/>
    </fragment>
    <fragment android:id="@+id/timeTableFragment" android:name="com.pocraft.gassai.view.fragment.TimeTableFragment"
              android:label="TimeTableFragment">
        <action android:id="@+id/action_timeTableFragment_to_teamFragment"
                app:destination="@id/teamFragment"/>
    </fragment>
    <fragment android:id="@+id/teamFragment" android:name="com.pocraft.gassai.view.fragment.TeamFragment"
              android:label="TeamFragment">
        <action android:id="@+id/action_teamFragment_to_reminderFragment"
                app:destination="@id/reminderFragment"/>
    </fragment>
    <fragment android:id="@+id/reminderFragment" android:name="com.pocraft.gassai.view.fragment.ReminderFragment"
              android:label="ReminderFragment"/>
</navigation>